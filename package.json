{
  "name": "eslint-config-nitpick",
  "version": "14.2.0",
  "description": "ESLint config for my projects.",
  "license": "MIT",
  "author": "Ivan NikoliÄ‡ <niksy5@gmail.com> (http://ivannikolic.com)",
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    },
    "./browser": {
      "types": "./index.d.ts",
      "import": "./browser.js"
    },
    "./tests": {
      "types": "./index.d.ts",
      "import": "./tests.js"
    },
    "./vue": {
      "types": "./index.d.ts",
      "import": "./vue.js"
    },
    "./typescript": {
      "types": "./index.d.ts",
      "import": "./typescript.js"
    },
    "./package.json": "./package.json"
  },
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "files": [
    "{index,browser,tests,vue,typescript}.js",
    "index.d.ts",
    "CHANGELOG.md",
    "LICENSE.md",
    "README.md"
  ],
  "scripts": {
    "lint": "eslint '{index,browser,tests,vue,typescript,test/index}.{js,d.ts}'",
    "prepare": "if [[ -x \"$(command -v husky)\" ]]; then husky; fi",
    "postpublish": "GITHUB_TOKEN=$GITHUB_RELEASE_TOKEN github-release-from-changelog",
    "prerelease": "npm run lint",
    "release": "np --no-release-draft",
    "test": "npm run lint && eslint-find-rules --flatConfig -u ./test/fixtures/all-rules.js && mocha 'test/index.js'",
    "test:watch": "npm test -- --watch",
    "version": "version-changelog CHANGELOG.md && changelog-verify CHANGELOG.md && git add CHANGELOG.md"
  },
  "dependencies": {
    "@typescript-eslint/parser": "^8.46.3",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^50.8.0",
    "eslint-plugin-mocha": "^10.5.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-unicorn": "^56.0.1",
    "eslint-plugin-vue": "^9.33.0",
    "globals": "^16.5.0",
    "resolve-from": "^5.0.0"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^18.19.130",
    "changelog-verify": "^1.1.2",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-find-rules": "^5.0.0",
    "eslint-plugin-prettier": "^5.5.4",
    "github-release-from-changelog": "^2.1.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "lodash": "^4.17.21",
    "mocha": "^10.8.2",
    "np": "^10.2.0",
    "prettier": "^3.6.2",
    "version-changelog": "^3.1.1"
  },
  "peerDependencies": {
    "eslint": "^9.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "nitpick"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/niksy/eslint-config-nitpick.git"
  },
  "bugs": {
    "url": "https://github.com/niksy/eslint-config-nitpick/issues"
  },
  "homepage": "https://github.com/niksy/eslint-config-nitpick#readme"
}
